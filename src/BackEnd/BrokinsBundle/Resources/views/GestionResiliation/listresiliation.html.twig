<center>



    <table class="table table-striped">
        <thead class="thead-dark">
        <tr>
            <td>Id</td>
            <td><h6>Origine Résiliation</h6></td>
            <td><h6>Date De Demande</h6></td>
            <td><h6>Raison De Resiliation</h6></td>
            <td><h6>Date Signature Du Contrat</h6></td>
            <td><h6>Date Echéance Du Contrat</h6></td>

            <td><h6>Possibilité De Résiliation</h6></td>
            <td><h6>Date De Raison De Résiliation</h6></td>
            <td><h6>Nouvelle Adresse</h6></td>

        </tr>
        </thead>
        {%  for modele in modeles %}

            <tr>
                <td>{{ modele.id }}</td>
                <td>{{ modele.origineResiliation }}</td>
                <td>{{ modele.dateDemande|date('m/d/y') }}</td>
                <td>{{ modele.idRaisonResiliation }}</td>
                <td>{{ modele.idContrat }}</td>
                <td>{{ modele.dateSignatureContrat|date('m/d/y') }}</td>
                <td>{{ modele.dateEcheanceContrat|date('m/d/y') }}</td>
                <td>{{ modele.possibiliteResiliation }}</td>
                <td>{{ modele.dateRaisonResiliation|date('m/d/y') }}</td>
                <td>{{ modele.nouvelleAdresse }}</td>
                <td>{{ modele.idPiece }}</td>
                <td>{{ modele.idPaiementPoste }}</td>
                <td><a href="{{ path('supprimerresiliation',{'id': modele.id }) }}" type="button" class="btn btn-danger">Supprimer</a></td>
            </tr>



        {% endfor %}
    </table>
